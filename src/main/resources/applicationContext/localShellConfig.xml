<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

  <bean class="com.nike.tools.bgm.tasks.LocalShellConfig">
    <qualifier value="createStageEnv"/>
    <property name="command" value="${bluegreen.localshell.createStageEnv.command}"/>
    <property name="regexpError" value="${bluegreen.localshell.createStageEnv.regexp.error}"/>
    <property name="exitvalueSuccess" value="${bluegreen.localshell.createStageEnv.exitvalue.success}"/>
  </bean>

  <bean class="com.nike.tools.bgm.tasks.LocalShellConfig">
    <qualifier value="deployPackages"/>
    <property name="command" value="${bluegreen.localshell.deployPackages.command}"/>
    <property name="exitvalueSuccess" value="${bluegreen.localshell.deployPackages.exitvalue.success}"/>
  </bean>

</beans>
